<cas:producttypes xmlns:cas="http://oodt.jpl.nasa.gov/1.0/cas">
  <type id="urn:edrn:eCASFile" name="eCASFile">
    <repositoryPath>file://[EDRN_REPO_HOME]/archive/misc</repositoryPath>
    <versionerClass>gov.nasa.jpl.oodt.cas.filemgr.versioning.MetadataBasedFileVersioner</versionerClass>
    <description>Unclassified eCAS data files.</description>
    <metExtractors>
      <extractor
        class="gov.nasa.jpl.oodt.cas.filemgr.metadata.extractors.CoreMetExtractor">
        <configuration>
          <!--  you can optionally include the envReplace tag to turn on/off environment var replacement -->
          <property name="nsAware" value="true" />
          <property name="elementNs" value="CAS" />
          <property name="elements"
            value="ProductReceivedTime,ProductName,ProductId" />
        </configuration>
      </extractor>
    </metExtractors>
    <metadata>
      <keyval>
        <key>Title</key>
        <val>Unclassified eCAS files.</val>
      </keyval>
      <keyval>
        <key>Date</key>
        <val>2008-06-24T20:12:00.000Z</val>
      </keyval>
      <keyval>
        <key>Author</key>
        <val>Unknown</val>
      </keyval>
      <keyval>
        <key>ProtocolId</key>
        <val>Unknown</val>
      </keyval>
      <keyval>
        <key>DatasetId</key>
        <val>urn:edrn:eCASFile</val>
      </keyval>
      <keyval>
        <key>SiteId</key>
        <val>Unknown</val>
      </keyval>
      <keyval>
        <key>OrganId</key>
        <val>Unknown</val>
      </keyval>
    </metadata>
  </type>
</cas:producttypes>
