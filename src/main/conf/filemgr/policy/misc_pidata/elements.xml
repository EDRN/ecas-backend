<?xml version="1.0" encoding="UTF-8"?>
<cas:elements xmlns:cas="http://oodt.jpl.nasa.gov/1.0/cas">
  
   <!--  CAS Specific Elements -->
   <element id="urn:oodt:ProductId" name="CAS.ProductId">
     <dcElement>Identifier</dcElement>
     <description>A Product's unique identifier within the CAS namespace.</description>   
   </element>
   <element id="urn:oodt:ProductName" name="CAS.ProductName">
     <dcElement>Title</dcElement>
     <description>A Product's name within the CAS namespace.</description>   
   </element>
   <element id="urn:oodt:ProductReceivedTime" name="CAS.ProductReceivedTime">
     <dcElement/>
     <description>The ISO 8601 formatted time that the Product was received.</description>   
   </element>
   <element id="urn:oodt:ProductType" name="ProductType">
     <dcElement/>
     <description>A Product's type classification.</description>   
   </element>
   <element id="urn:oodt:Filename" name="Filename">
     <dcElement>Title</dcElement>
     <description>A potentially multi-valued element capturing the filename(s) of the files within this product.</description>   
   </element>
   <element id="urn:oodt:FileLocation" name="FileLocation">
     <dcElement/>
     <description>The location (directory) that the file resides in.</description>   
   </element>

