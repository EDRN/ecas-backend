#!/bin/tcsh
# Copyright (c) 2008 California Institute of Technology.
# ALL RIGHTS RESERVED. U.S. Government Sponsorship acknowledged.
#
# $Id$

set ORIG_DIR = $cwd
set DIR = `dirname $0`
cd $DIR
set DIR_PATH = `pwd`

if ( $#argv != 2 ) then
   echo "Usage: $0 <filemgr url> <data dir path>"
   exit 1
else
        cd $DIR_PATH/../crawler/bin
        ./crawler_launcher \
        --crawlerId MetExtractorProductCrawler \
        --metExtractor gov.nasa.jpl.edrn.ecas.metextraction.SELDIMetExtractor \
        --metExtractorConfig $EDRN_ECAS_HOME/aux/met_def/ecas_metdb_fhcrc.csv \
        --filemgrUrl $1 \
        --productPath $2 \
        --failureDir /data/failed_ingest_files \
        --metFileExtension met \
        --actionIds DeleteDataFile,MoveDataFileToFailureDir,Unique
endif
